{
  "name": "cypress-start",
  "version": "1.2.0",
  "description": "Test automation framework based on Cypress.io for web applications.",
  "repository": {
    "type": "git",
    "url": "https://github.com/IvanZdanovich/cypress-start"
  },
  "scripts": {
    "pretest": "node cypress/support/scripts/copy-localization.js && node cypress/support/scripts/copy-colours-theme.js",
    "test": "cypress run --headless",
    "lint": "eslint . --format stylish --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "node .husky/check-eslint.js"
    ]
  },
  "private": false,
  "keywords": [],
  "author": "Ivan Zdanovich",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "cypress": "^15.5.0",
    "mochawesome": "^7.1.4"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
