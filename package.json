{
  "name": "cypress-start",
  "version": "1.2.3",
  "description": "Cypress testing framework starter - Professional test automation framework template with comprehensive structure, documentation, and best practices",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IvanZdanovich/cypress-start.git"
  },
  "bin": {
    "cypress-start": "bin/cypress-start.js",
    "create-cypress-start": "bin/cypress-start.js"
  },
  "files": [
    "bin/",
    "cypress/e2e/",
    "cypress/integration/",
    "cypress/support/",
    "cypress/test-data/",
    "cypress/sensitive-data/env-users.example.json",
    "cypress/reports/mochawesome/.gitkeep",
    "cypress/reports/screenshots/.gitkeep",
    "docs/",
    "app-structure/",
    "bug-log/",
    "development-data/",
    "eslint-plugin-custom-rules/",
    "scripts/",
    ".github/copilot-instructions.md",
    ".github/instructions/",
    ".github/FUNDING.yml",
    "assets/",
    "cypress.config.js",
    "eslint.config.mjs",
    "Dockerfile",
    ".gitignore",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "pretest": "node scripts/copy-localization.js && node scripts/copy-colours-theme.js",
    "test": "cypress run --headless",
    "test:parallel": "node scripts/parallel-cypress-runner.js",
    "lint": "eslint . --format stylish --fix",
    "hooks:setup": "node scripts/setup-git-hooks.js",
    "postinstall": "node scripts/setup-git-hooks.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "node scripts/check-eslint.js"
    ]
  },
  "private": false,
  "keywords": [
    "cypress",
    "testing",
    "framework",
    "template",
    "starter",
    "boilerplate",
    "test-automation",
    "e2e",
    "integration-testing",
    "ui-testing",
    "api-testing",
    "cypress-framework",
    "cypress-template",
    "test-framework"
  ],
  "author": "Ivan Zdanovich",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^13.0.0",
    "prettier": "^3.7.3"
  },
  "dependencies": {
    "cypress": "^15.7.1",
    "mochawesome": "^7.1.4"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
